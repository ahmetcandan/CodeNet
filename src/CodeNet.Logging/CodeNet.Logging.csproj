<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net8.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<GeneratePackageOnBuild>True</GeneratePackageOnBuild>
		<IncludeSymbols>False</IncludeSymbols>
		<PackAsTool>False</PackAsTool>
		<PackageId>$(AssemblyName)</PackageId>
		<PackageIcon>ico.png</PackageIcon>
		<Title>CodeNet Logging</Title>
		<Description>CodeNet Logging package.</Description>
		<Copyright>Copyright 2024</Copyright>
		<Company>CodeNet</Company>
		<Version>1.0.3</Version>
		<Authors>CodeNet</Authors>
		<PackageReadmeFile>README.md</PackageReadmeFile>
		<PackageProjectUrl>https://github.com/ahmetcandan/CodeNet_Example</PackageProjectUrl>
	</PropertyGroup>

	<ItemGroup>
		<None Include="..\..\ico.png">
			<Pack>True</Pack>
			<PackagePath>\</PackagePath>
		</None>
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
	</ItemGroup>

	<ItemGroup>
		<ProjectReference Include="..\CodeNet.Container\CodeNet.Container.csproj" />
		<ProjectReference Include="..\CodeNet.Core\CodeNet.Core.csproj" />
	</ItemGroup>

</Project>
